{
  "expo": {
    "name": "Audtiklance",
    "owner": "audtiklance",
    "slug": "audtiklance_mobile",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/logo.jpeg",
    "scheme": "audtiklance",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "MAPBOX_KEY": "pk.eyJ1IjoidmlrdG9oIiwiYSI6ImNrcmpyamM1ZjA1ZG8ydnBjbWRpOWtjN2kifQ.auBvgsbud2l08nrj8lXZfg",
      "BASE_API": "https://api.audtiklance.com",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "99a4064b-a946-47ac-b868-2346aead1e58"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "audtiklance",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to show nearby service providers and personalized recommendations. For example, this helps us find service providers within 5 miles of you.",
        "NSPhotoLibraryUsageDescription": "We need access to your photo library so you can upload service images and profile pictures.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.jpeg",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "resize",
      "package": "com.richey24.audtiklance_mobile",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.jpeg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.jpeg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Audtiklance to use your location."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.15571279761-v4q3rb97hueq6koviotj3fa3jflhvi4p"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/99a4064b-a946-47ac-b868-2346aead1e58"
    }
  }
}
